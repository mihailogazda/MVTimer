<Bindings>
  <Layer name="General">
    <Category name="MVTimer Settings">
      <Action name="ACTIVATE_POWERUP_1">
        <Down>MVTimer.HandleKeyDown(0)</Down>
		<Up>MVTimer.HandleKeyUp(0)</Up>
      </Action>
      <Action name="ACTIVATE_POWERUP_2">
        <Down>MVTimer.HandleKeyDown(1)</Down>
		<Up>MVTimer.HandleKeyUp(1)</Up>
      </Action>
	  <Action name="ACTIVATE_POWERUP_3">
        <Down>MVTimer.HandleKeyDown(2)</Down>
		<Up>MVTimer.HandleKeyUp(2)</Up>
      </Action>
      <Action name="ACTIVATE_POWERUP_4">
        <Down>MVTimer.HandleKeyDown(3)</Down>
		<Up>MVTimer.HandleKeyUp(3)</Up>
      </Action>	  
    </Category>
  </Layer>
</Bindings>
