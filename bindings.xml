<Bindings>
  <Layer name="General">
    <Category name="|cEB609E|MVTimer Settings">
      <Action name="ACTIVATE_TIMER">
        <Down>MVTimer.HandleKeyDown(KEY_TIMER_START)</Down>
		<Up>MVTimer.HandleKeyUp(KEY_TIMER_START)</Up>
      </Action>	  
      <Action name="DISABLE_TIMER">
        <Down>MVTimer.HandleKeyDown(KEY_TIMER_STOP)</Down>
		<Up>MVTimer.HandleKeyUp(KEY_TIMER_STOP)</Up>
      </Action>	
    </Category>
  </Layer>
</Bindings>
